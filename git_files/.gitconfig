[user]
  name = Rick Winfrey
  email = rewinfrey@github.com

[core]
  editor = /usr/local/bin/vim
  excludesfile = ~/.gitignore

[color]
  ui = true

[color "diff"]
  whitespace = red reverse

[alias]
  diff = diff --patience --ignore-space-change
  show = show --patience --ignore-space-change
  co = checkout
  st = status
  br = branch
  shas = log --pretty=format:\"%H\"
  hist = log --pretty=format:\"%Cgreen%h%Creset %Cblue%ad%Creset %s%C(yellow)%d%Creset %Cblue[%an]%Creset\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  praise = blame
  count = git shortlog -sn
	unstage = reset HEAD --
	last = log -1 HEAD

[push]
  default = upstream

[hub]
  protocol = https
[rerere]
	enabled = true
[credential]
	helper = osxkeychain
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
